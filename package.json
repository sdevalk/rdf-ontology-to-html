{
  "name": "rdf-ontology-to-html",
  "description": "Package for generating HTML from an RDF ontology",
  "version": "0.0.5",
  "author": "Sjors de Valk <sjors@sjorsdevalk.nl>",
  "bin": "./bin/run",
  "engines": {
    "node": ">=10.0.0"
  },
  "private": true,
  "scripts": {
    "lint": "lab -L",
    "test": "lab -a @hapi/code -t 100 -L",
    "test-cov-html": "lab -a @hapi/code -r html -o coverage.html"
  },
  "dependencies": {
    "debug": "4.1.1",
    "n3": "1.1.1",
    "fs-extra": "7.0.1",
    "globby": "9.2.0",
    "handlebars": "4.5.3",
    "@hapi/joi": "15.1.0",
    "@hapi/wreck": "15.0.1",
    "humanize-string": "2.1.0",
    "@oclif/command": "1.5.14",
    "@oclif/config": "1.13.0",
    "@oclif/errors": "1.2.2",
    "@oclif/plugin-help": "2.1.4",
    "titleize": "2.1.0"
  },
  "devDependencies": {
    "@hapi/code": "5.3.1",
    "@hapi/lab": "18.1.2",
    "pkg": "4.4.0"
  },
  "oclif": {
    "commands": "./lib/commands"
  },
  "files": [
    "bin",
    "lib",
    "templates"
  ]
}
